reconfiguration insertP(Pattern p; Node n, mi, mo, j1, j2) {
	Pattern p1 = @ par(p);
	Pattern p2 = @ split(n);
	Set<Node> n1 = p2.in - p1.in ;
	Set<Node> n2 = p2.out - p1.out ;
	Set<Node> E1 = n1 + S(mi);
	@ join(E1);
	Set<Node> E2 = n2 + S(mo);
	@ join(E2);  
}

reconfiguration removeP	(Set<Name> Cs ) {
	forall ( Name n : Cs)
	{	
		@ remove(n);
	}
}